  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.18.2",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
