{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "plugins": ["unused-imports"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "unused-imports/no-unused-imports": "warn",
    "no-unused-vars": "warn",
    "no-empty": "off"
  }
}
